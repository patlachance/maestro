<div style="text-align: left;">
  <h4 class="help-title">Help</h4>
  <h3 style="font-size: 17px;color:white;">Get Started</h3>
  <div>
      <% if(documentation !== undefined && documentation !== null){ %>
        <% documentation.forEach(function(doc){ %>
          <% if(doc.category == 'tutorial'){ %>
            <%if(doc.url === '#'){%>
                <a style="display: block;font-size: 13px;"><%=doc.dname%> [N/A]</a>
            <% }else{ %>
                <a href="<%=doc.url%>" target="_blank" style="display: block;font-size: 13px;"><%=doc.dname%></a>
            <% } %>
          <% } %>
        <% }); %>
      <% } %>
  </div>
